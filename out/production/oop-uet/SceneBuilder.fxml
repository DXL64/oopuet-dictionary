                        <ListView fx:id="listViewShowWord" layoutX="200.0" layoutY="138.0" prefHeight="282.0" prefWidth="495.0" />
                        <VBox fx:id="vBoxAddNewWord" layoutX="200.0" layoutY="57.0" prefHeight="83.0" prefWidth="209.0" visible="false">
                            <children>
                                <TextField fx:id="textFieldAddEnglish" prefHeight="25.0" prefWidth="203.0" promptText="English" />
