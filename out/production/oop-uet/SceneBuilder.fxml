                            <image>
                                <Image url="@findIcon.png" />
                            </image>
                        </ImageView>
                        <ImageView fitHeight="25.0" fitWidth="25.0" layoutX="669.0" layoutY="1.0" onMouseClicked="#onClickDeleteIcon" pickOnBounds="true" preserveRatio="true">
