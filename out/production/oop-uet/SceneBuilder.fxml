        </Tab>
        <Tab text="Google Translate">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
        </Tab>
        <Tab fx:id="tab" text="Multi Dictionary">
         <content>
            <BorderPane prefHeight="200.0" prefWidth="200.0">
               <center>
                  <VBox fx:id="vBoxMultidict" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER" />
               </center></BorderPane>
         </content></Tab>
